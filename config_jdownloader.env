# JDownloader Configuration for MEGA Downloads
# Copy this file to config.env and fill in your values

# JDownloader Settings
JDOWNLOADER_HOST=http://localhost:5800
JDOWNLOADER_USERNAME=your_jdownloader_username
JDOWNLOADER_PASSWORD=your_jdownloader_password
JDOWNLOADER_DEVICE_ID=your_device_id_optional

# MEGA Account Settings (for premium features)
MEGA_USERNAME=your_mega_email
MEGA_PASSWORD=your_mega_password

# Download Settings
DOWNLOAD_DIR=/downloads
DOWNLOAD_STATUS_UPDATE_INTERVAL=5

# Bot Settings
BOT_TOKEN=your_telegram_bot_token
OWNER_ID=your_telegram_user_id
GDRIVE_FOLDER_ID=your_google_drive_folder_id

# Redis Settings (required)
REDIS_HOST=your_redis_host
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# Telegram API Settings
TELEGRAM_API=your_telegram_api_id
TELEGRAM_HASH=your_telegram_api_hash
USER_SESSION_STRING=your_session_string

# Optional Settings
IS_TEAM_DRIVE=False
USE_SERVICE_ACCOUNTS=False
INDEX_URL=
UPLOAD_AS_VIDEO=false
VIDEO_THUMB_PATH=
USE_CUSTOM_THUMB=false
TG_PART_SIZE_MB=1950